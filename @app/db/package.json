{
  "name": "@starter/db",
  "private": true,
  "version": "0.0.0",
  "description": "The Prisma DB Migrations for the Starter",
  "scripts": {
    "gm": "node node_modules/.bin/graphile-migrate",
    "migrate": "node node_modules/.bin/graphile-migrate migrate",
    "watch": "yarn gm watch",
    "commit": "yarn gm commit",
    "uncommit": "yarn gm uncommit",
    "reset": "yarn gm reset"
  },
  "dependencies": {
    "graphile-migrate": "^1.2.0"
  },
  "devDependencies": {
    "@types/pg": "^8.6.1",
    "pg": "^8.7.1"
  }
}
